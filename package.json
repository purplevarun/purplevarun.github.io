{
    "name": "purplevarun.github.io",
    "private": true,
    "version": "1.0.0",
    "homepage": "https://purplevarun.github.io",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "test": "vitest",
        "coverage": "rm -rf coverage && vitest run --coverage",
        "format": "prettier . --write",
        "cleanup": "rm -rf node_modules coverage yarn.lock && yarn",
        "prepreview": "yarn clean && yarn build",
        "preview": "vite preview",
        "build": "tsc && vite build",
        "postbuild": "cp dist/index.html dist/404.html",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
    },
    "dependencies": {
        "@testing-library/react": "^14.1.2",
        "framer-motion": "^10.16.16",
        "ismobilehook": "^1.4.0",
        "rainbow-text-react": "2.3.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-icons": "^4.12.0",
        "sass": "^1.69.6"
    },
    "devDependencies": {
        "@types/react": "^18.2.43",
        "@types/react-dom": "^18.2.17",
        "@typescript-eslint/eslint-plugin": "^6.14.0",
        "@typescript-eslint/parser": "^6.14.0",
        "@vitejs/plugin-react": "^4.2.1",
        "@vitest/coverage-v8": "^1.1.1",
        "eslint": "^8.55.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "jsdom": "^23.0.1",
        "prettier": "3.1.1",
        "typescript": "^5.2.2",
        "vite": "^5.0.8",
        "vitest": "^1.1.1"
    }
}
